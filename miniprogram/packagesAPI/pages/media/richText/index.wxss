/* packagesAPI/pages/media/richText/index.wxss */
.editor-example-fixed {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 340rpx;
  background-color: #fff;
  z-index: 10;
}

.editor-example-area {
  width: 90%;
  height: 300rpx;
  border: #000 solid 4rpx;
  margin: 10rpx auto;
  padding: 10rpx 20rpx;
  border-radius: 20rpx;
  display: block;
  position: relative;
  box-sizing: border-box;
}

.editor-content {
  min-height: 98%;
  height: 98%;
  box-sizing: border-box;
}

.editor-example-block {
  width: 100%;
  height: 340rpx;
}
